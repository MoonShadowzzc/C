double mypow(double x, int y)
{
	double z = 1.0; 
	int i = 0;
	for(i = 1 ; i <= y; ++i)
	{
		z *= x;
	}
	return z;
}

int myfac(int n)
{
	int i = 0, z = 1;
	for(i=n; i>0; i--)
		z *= i;
	return z;
}

